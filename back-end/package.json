{
  "name": "back-end",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma-migrate-dev": "npx prisma migrate dev --preview-feature",
    "prisma-migrate-reset": "npx prisma migrate reset --preview-feature",
    "prisma-studio": "npx prisma studio",
    "prisma-generate": "npx prisma generate",
    "prisma-codemods": "npx @prisma/codemods update-2.12 ./",
    "update-db": "npm run prisma-generate && npm run prisma-migrate-dev",
    "docker-stop-containers": "npx run-script-os",
    "docker-stop-containers:windows": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./bin/DockerStopContainers/DockerStopContainers.ps1",
    "docker-stop-containers:default": "docker stop $(docker ps -a -q)",
    "run-db": "npm run docker-stop-containers && docker-compose up",
    "start": "node ./build/index.js",
    "dev": "npm run update-db && npx nodemon --exec npx babel-node index.js",
    "devWithChangedConfig": "npm run update-db && npx nodemon --exec npx babel-node index.js",
    "prettier": "npx prettier --config ./.prettierrc --ignore-path ./.prettierignore --loglevel warn --write './**/*.js'",
    "fix": "npm run prettier && (npx flow & npx eslint . --fix)",
    "deps": "npx run-script-os",
    "deps:windows": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./bin/Deps/Deps.ps1",
    "deps:default": "sh ./bin/Deps/Deps.sh",
    "help": "npx babel-node bin/Help",
    "clean": "npx rimraf ./build",
    "build": "npm run clean && npx babel . --out-dir ./build --ignore 'node_modules/*','flow-typed/*','bin/*'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@prisma/cli": "^2.15.0",
    "@prisma/client": "^2.21.2",
    "@sendgrid/mail": "^7.4.0",
    "algoliasearch": "^4.3.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "firebase": "^7.24.0",
    "fs-extra": "^9.0.1",
    "http": "0.0.1-security",
    "ip": "^1.1.5",
    "lodash": "^4.17.19",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "random-key": "^0.3.2",
    "redis": "^3.1.1",
    "socket.io": "^2.4.1",
    "util": "^0.12.3",
    "xhr2": "^0.2.0"
  },
  "devDependencies": {
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-flow": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "flow-bin": "^0.130.0",
    "flow-typed": "^3.2.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "prisma": "^2.21.2",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.1"
  }
}
