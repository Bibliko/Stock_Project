{
  "name": "back-end",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma-save": "prisma migrate save --experimental",
    "prisma-up": "prisma migrate up --experimental",
    "prisma-studio": "prisma studio --experimental",
    "prisma-generate": "npx prisma generate && babel-node ./bin/PrismaFlowTypedGenerator",
    "update-db": "npm run prisma-generate && npm run prisma-save && npm run prisma-up",
    "update-db-ps": "npm run prisma-generate && npm run prisma-save && npm run prisma-up",
    "docker-stop-containers": "docker stop $(docker ps -a -q)",
    "docker-stop-containers-ps": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./powershell-scripts/docker-stop-containers.ps1",
    "run-db": "npm run docker-stop-containers && docker-compose up",
    "run-db-ps": "npm run docker-stop-containers-ps && docker-compose up",
    "start": "node ./build/index.js",
    "dev": "node checkConfigFolder.js && node checkChangesInSchemaPrisma.js && npm run update-db && nodemon --exec babel-node index.js",
    "prettier": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --loglevel warn --write './**/*.js'",
    "fix": "npm run prettier && (flow & eslint . --fix)",
    "deps": "npm install --save $npm_config_package && flow-typed install $npm_config_package",
    "help": "babel-node displayNpmHelp.js",
    "clean-build": "rm -rf ./build",
    "build": "npm run clean-build && babel . --out-dir ./build --ignore 'node_modules/*','flow-typed/*'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/cli": "^2.3.0",
    "@prisma/client": "^2.3.0",
    "@sendgrid/mail": "^7.2.1",
    "algoliasearch": "^4.3.0",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "firebase": "^7.16.0",
    "fs-extra": "^9.0.1",
    "http": "0.0.1-security",
    "ip": "^1.1.5",
    "lodash": "^4.17.19",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "random-key": "^0.3.2",
    "redis": "^3.0.2",
    "socket.io": "^2.3.0",
    "util": "^0.12.3",
    "xhr2": "^0.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-flow": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "flow-bin": "^0.130.0",
    "flow-typed": "^3.2.1",
    "prettier": "^2.0.5"
  }
}
