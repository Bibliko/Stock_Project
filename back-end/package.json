{
  "name": "back-end",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma-save": "prisma migrate save --experimental",
    "prisma-up": "prisma migrate up --experimental",
    "prisma-studio": "prisma studio --experimental",
    "update-db": "npx prisma generate && npm run prisma-save && npm run prisma-up",
    "update-db-ps": "npx prisma generate && npm run prisma-save && npm run prisma-up",
    "docker-stop-containers": "docker stop $(docker ps -a -q)",
    "docker-stop-containers-ps": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./powershell-scripts/docker-stop-containers.ps1",
    "run-db": "npm run docker-stop-containers && docker-compose up",
    "run-db-ps": "npm run docker-stop-containers-ps && docker-compose up",
    "dev": "nodemon index.js",
    "start": "node index.js"
  },
  "keywords": [],
